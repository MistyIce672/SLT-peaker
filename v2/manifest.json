{

    "manifest_version": 2,
    "name": "SLT DATA PEAKER",
    "version": "1.0",
  
    "description": "Improves slt blanace dashboard by showing remaining data and off peak separately ",
  
    "icons": {
      "48": "icons/icon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://myslt.slt.lk/*"],
        "js": ["app.js"]
      }
    ],
    "browser_action": {
        "default_popup": "ui/login.html"
    },
    "permissions": [
        "storage"
    ]
  
  }
  